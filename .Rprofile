# to load_all or document, you must first run devtools::unload("parsnip")

load_all <- function() {
  if (R.utils::isPackageLoaded("parsnip")) devtools::unload("parsnip")
  devtools::load_all()
}

document <- function() {
  if (R.utils::isPackageLoaded("parsnip")) devtools::unload("parsnip")
  devtools::document()
}
